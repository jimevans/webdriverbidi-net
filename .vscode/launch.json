    "version": "0.2.0",
    "configurations": [
        {
            "name": "WebDriverBiDi.Demo Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/src/WebDriverBiDi.Demo/bin/Debug/net6.0/WebDriverBiDi.Demo.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/WebDriverBiDi.Demo",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "stopAtEntry": false
        },
        {
            "name": "WebDriverBiDi.DemoWebSite Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/src/WebDriverBiDi.DemoWebSite/bin/Debug/net6.0/WebDriverBiDi.DemoWebSite.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/WebDriverBiDi.DemoWebSite",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
